<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.portfo.web.repository.PortfolioSkillRepository">
    <select id="findByPortfolioId" resultType="PortfolioSkill">
        SELECT id, eng_name, kor_name, portfolio_id
        FROM skill
        WHERE portfolio_id = #{portfolioId}
    </select>
</mapper>